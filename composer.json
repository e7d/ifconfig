{
  "name": "e7d/ifconfig",
  "description": "Get you connection information.",
  "autoload": {
    "classmap": [
      "app/src/"
    ]
  },
  "require": {
    "geoip2/geoip2": "^2.10",
    "symfony/yaml": "^5.0"
  },
  "scripts": {
    "benchmark": "gb -c 100 -n 100000 -z -k http://localhost:5757",
    "build:docker": "test -f .env && . ./.env && docker build --build-arg MAXMIND_LICENSE_KEY=$MAXMIND_LICENSE_KEY -t ifconfig .",
    "setup": "composer install && composer dump-autoload"
  }
}
